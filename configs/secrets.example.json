{
  "discord": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
      "token": "discord.token"
    }
  ],
  "twitch": [
    {
      "name": "name.for.in.logs",
      "enabled": true,
      "secrets": {
        "token": "bot.token",
        "id": "bot.id.optional",
        "secret": "bot.secret.optional",
        "refresh": "bot.refresh.token.optional",
        "redirectUri": "bot.oauth.url.callback",
        "scopes": "chat:read chat:edit channel:moderate"
      },
      "settings": {
        "channel": "streamer.name",
        "username": "bot.username",
        "prefix": "command.prefix.!.optional",
        "superusers" : [
          "streamer.or.dev.or.maintainer.username.goes.here.optional"
        ],
        "usersIgnore": [
          "sery_bot",
          "soundalerts",
          "optional"
        ],
        "commandsIgnore": [
          "optional"
        ],
        "systemsIgnore": [
          "optional"
        ]
      }
    }
  ],
  "console": {
    "port": 8080
  }
}
